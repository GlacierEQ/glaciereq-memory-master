{
  "mcpServers": {
    "memory-orchestrator": {
      "command": "python",
      "args": ["core/memory-orchestrator/server.py"],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "${NEO4J_PASSWORD}",
        "SUPERMEMORY_API_KEY": "${SUPERMEMORY_API_KEY}",
        "MEM0_API_KEY": "${MEM0_API_KEY}"
      }
    }
  }
}
